<Window x:Class="OnlineCourseApp.AdminLogsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Журнал дій адміністратора" Height="400" Width="600"
        WindowStartupLocation="CenterOwner" Background="#FAFAFA">
    <StackPanel Margin="10">
        <TextBlock Text="Журнал дій" FontSize="18" FontWeight="Bold" Margin="0 0 0 10"/>
        <ListBox x:Name="LogsList" />
       <!-- <Button Content="Переглянути журнал дій" Margin="5" Command="{Binding ViewLogsCommand}"/> -->
        <Button Content="Закрити" HorizontalAlignment="Right" Width="100" Margin="0 10 0 0" Click="Close_Click"/>
    </StackPanel>
</Window>

